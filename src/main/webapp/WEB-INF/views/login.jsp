

<div class="row">
  <div class="span4">&nbsp;</div>
  <div class="span4">
        <h2>Login</h2>
      <form class="form-horizontal" action="j_spring_security_check" method="post" id="loginForm">
        <label>Username</label>
        <input class="text" type="text" name="j_username" id="j_username" />
        <label>Password</label>
        <input class="text" type="password" name="j_password" id="j_password"/>
        <label class="checkbox">
            Remember me on this computer.
            <input style="" type='checkbox' name='_spring_security_remember_me'/>
        </label>
        <button class="btn btn-primary" type="submit">login</button>
    </form>

  </div>
</div>


